{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-prototype-builtins": 0,
    "no-underscore-dangle": 0,
    "no-restricted-syntax": 0
  },
  "ignorePatterns": ["public/"]
}